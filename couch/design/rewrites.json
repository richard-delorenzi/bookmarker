[
    {
	"from": "favicon.ico",
	"to": "images/blackboard.png"
    },
    {
	"from" : "script/*",
	"to" : "script/*"
    },
    { 
	"from" : "utils_script",
	"to" :   "/_utils/script/*"
    },
    {
	"from" : "style/*",
	"to" : "style/*"
    },
    {
	"from" : "vendor/*",
	"to" : "vendor/*"
    },
    {   
        "from": "server_lib/:~name",
        "to": "_list/sub/design",
        "query": {
	    "~dir": "lib",	    
	    "include_docs": "true"
        }
    },
    { 
	"from" : "index",
	"to" : "_list/mustache-only/none",
	"query": {
	    "template":"index"
	}
    },
    {
	"from" : "",
	"to"   : "_rewrite/index" 
    },
    {
	"from" : "add",
	"to":    "_list/add-bookmark/webmarks-by-tag",
	"query": {
	    "template": "add-bookmark",
	    "group": "true",
	    "title_part1": "add",
	    "title_part2": "bookmark"
	}
    },
    {
	"from": "bookmarks",
        "to": "_list/bookmarks/webmarks-by-date",
	"query": {
	    "template": "bookmarks",
	    "reduce": "false",
	    "title_part1": "All",
	    "title_part2": "bookmarks",
	    "include_docs": "true",
	    "descending" : "true"
	}	
    },
    {
	"from": "tag/:~tagname",
        "to": "_list/bookmarks/webmarks-by-tag",
	"query": {
	    "template": "bookmarks",
	    "reduce": "false",
	    "title_part1": ":~tagname",
	    "title_part2": "tags",
	    "key": [":~tagname"],
	    "include_docs": "true"
	}	
    },
    {
	"from": "tags",
        "to": "_list/tags/webmarks-by-tag",
	"query": {
	    "template": "tags",
	    "group" : "true",
	    "title_part1": "all",
	    "title_part2": "tags"
	}	
    },
    
    {
	"from": "db/*",
	"to": "../../*"
    }
]
