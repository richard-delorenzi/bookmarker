[
    {
	"from": "favicon.ico",
	"to": "images/stars.png"
    },
    {
	"from" : "script/*",
	"to" : "script/*"
    },
    { 
	"from" : "utils_script",
	"to" :   "/_utils/script/*"
    },
    {
	"from" : "style/*",
	"to" : "style/*"
    },
    {
	"from" : "images/*",
	"to" : "images/*"
    },
    {
	"from" : "vendor/*",
	"to" : "vendor/*"
    },
    {   
        "from": "server_lib/:~name",
        "to": "_list/sub/design",
        "query": {
	    "~dir": "lib",	    
	    "include_docs": "true"
        }
    },
    {
	"from": "_layout",
        "to": "_list/mustache-only/none",
	"query": {
	    "template": "layout"
	}	
    },
    { 
	"from" : "index",
	"to" : "_list/mustache-only/none",
	"query": {
	    "template":"index"
	}
    },
    {
	"from" : "",
	"to"   : "_rewrite/index" 
    },
    {
	"from" : "login",
	"to"   : "../../../login/_design/auth/_rewrite" 
    },
    {
	"from" : "add",
	"to":    "_show/add-bookmark",
	"query": {
	    "template": "add-bookmark",
	    "title_part1": "Add",
	    "title_part2": "bookmark"
	}
    },
    {
	"from" : "edit/:~id",
	"to":    "_show/add-bookmark/:~id",
	"query": {
	    "template": "add-bookmark",
	    "title_part1": "Edit",
	    "title_part2": "bookmark"
	}
    },
    {
	"from": "_bookmarks_base",
        "to": "_list/bookmarks/webmarks-by-date",
	"query": {
	    "template": "bookmarks",
	    "reduce": "false",
	    "include_docs": "true",
	    "descending" : "true"
	}	
    },
    {
	"from": "bookmarks",
        "to": "_rewrite/_bookmarks_base",
	"query": {
	    "title_part1": "All",
	    "title_part2": "bookmarks"
	}	
    },
    {
	"from": "latest-bookmarks",
        "to": "_rewrite/_bookmarks_base",
	"query": {
	    "title_part1": "Latest",
	    "title_part2": "bookmarks",
	    "limit": "100"
	}	
    },
    {
	"from": "_bookmarks_by_url",
        "to": "_list/bookmarks/webmarks-by-url",
	"query": {
	    "title_part1": "Bookmarks",
	    "title_part2": "by url",
	    "template": "bookmarks",
	    "reduce": "false",
	    "include_docs": "true",
	    "descending" : "false"
	}	
    },
    {
	"from": "tag/:~tagname",
        "to": "_list/bookmarks/webmarks-by-tag",
	"query": {
	    "template": "bookmarks",
	    "title_part1": ":~tagname",
	    "title_part2": "tags",
	    "mode": "tags",
	    "reduce": "false",
	    "startkey": [":~tagname",{}],
	    "endkey": [":~tagname"],
	    "include_docs": "true",
	    "descending" : "true"
	}	
    },
    {
	"from": "_tags_base",
        "to": "_list/tags/webmarks-by-tag",
	"query": {
	    "template": "tags",
	    "group_level" : "1"
	}	
    },
    {
	"from": "tags",
        "to": "_rewrite/_tags_base",
	"query": {
	    "title_part1": "all",
	    "title_part2": "tags"
	}	
    },
    {
	"from": "db/*",
	"to": "../../*"
    }
]
